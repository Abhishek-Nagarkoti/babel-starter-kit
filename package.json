        "babel-preset-stage-0": "^6.16.0",
        "babel-register": "^6.18.0",
        "bs-html-injector": "^3.0.3",
        "chai": "^3.5.0",
        "coveralls": "^2.11.14",
        "del": "^2.2.2",
        "ecstatic": "^2.1.0",
        "eslint": "^3.9.1",
        "eslint-config-airbnb-base": "^9.0.0",
        "eslint-plugin-import": "^2.1.0",
        "gulp-add-less-variables": "^1.0.4",
        "gulp-autoprefixer": "^3.1.1",
