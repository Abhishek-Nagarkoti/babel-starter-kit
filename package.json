        "eslint-plugin-import": "^2.1.0",
        "istanbul": "^0.4.5",
        "jquery": "^3.1.1",
        "jquery-rest-client": "git+https://github.com/hiteshjoshi/jquery-rest-client.git",
        "js-cookie": "^2.1.3",
        "moment": "^2.15.2",
        "page": "^1.7.1",
        "restful.js": "^0.9.6",
        "sinon": "^1.17.6"
    },
    "devDependencies": {
        "babel-cli": "^6.18.0",
        "babel-core": "^6.18.2",
