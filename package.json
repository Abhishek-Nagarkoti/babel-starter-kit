{
    "private": true,
    "name": "babel-starter-kit",
    "version": "0.0.0",
    "description": "A project template for authoring and publishing JavaScript libraries",
    "homepage": "https://www.kriasoft.com/babel-starter-kit/",
    "repository": "hiteshjoshi/babel-starter-kit",
